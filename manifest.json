{
    "manifest_version": 2,
    "name": "Hellfest Ticket Finder",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://www.ticketswap.fr/*"
            ],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "trophy.png"
      },
      "background": {
        "scripts": ["background.js"]
      }
}